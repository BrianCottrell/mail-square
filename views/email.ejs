<html lang="en">
  <head>
    <meta charset="utf-8">
    <title>MailSquare</title>
    <link rel="stylesheet" href="css/style.css">
    <link rel="stylesheet" href="https://ajax.googleapis.com/ajax/libs/jqueryui/1.11.4/themes/smoothness/jquery-ui.css">
     <script src="https://ajax.googleapis.com/ajax/libs/jqueryui/1.11.4/jquery-ui.min.js"></script>
      <script src="https://code.jquery.com/jquery-1.11.1.js"></script>
  </head>
  <body>
    <p>Email Page</p>
    <% var emailData = JSON.parse(email.content) %>
    <%= emailData.sender %>
    <%= emailData.date %>
    <%= index %>
    <div class='content'><%- emailData.content %></div>
    <div class='htmlContent'></div>
    <a href=<%= '/email/:sender='+email.sender+'/:index='+(parseInt(index)+1) %>>next</a>
    <a href=<%= '/email/:sender='+email.sender+'/:index='+(parseInt(index)-1) %>>Previous</a>
    <script src='email.js' type='text/javascript'></script>
  </body>
</html>